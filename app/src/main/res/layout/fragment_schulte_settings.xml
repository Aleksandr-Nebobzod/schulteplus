<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".ui.schultesettings.SchulteSettingsFragment">
	
	<TextView
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_marginTop="10dp"
		android:text="there are prestart settings of Schulte tables training should be soon" />
	
	<EditText
		android:id="@+id/et_x_size"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:minHeight="48dp"
		android:imeOptions="actionNext"
		android:layout_marginHorizontal="10dp"
		android:layout_marginTop="100dp"
		android:hint="@string/hnt_input_width" />
	
	<EditText
		android:id="@+id/et_y_size"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:minHeight="48dp"
		android:imeOptions="actionNext"
		android:layout_marginTop="200dp"
		android:layout_marginHorizontal="10dp"
		android:hint="@string/hnt_input_height" />

</FrameLayout>